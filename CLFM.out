        FOREACH YY IN(#KK)
          .IF(YY.rec = 1,*SOURCE,       ) ? CRNALL ? ? ? ? ? ? ? ? ? ? ? ? ? .REPLACE(.replace(YY,"[","("),"]",")") _AREA .IF(YY.rec < YY.count,+)
        ENDFOR
      *OUTPUT ohvKKHaPrvbyEU.IF(XX=NULL,,tXX)(_TH1) Area Harvested (KK Actions) - ha, Owner: Private, Th1Based
        FOREACH YY IN(#KK)
          .IF(YY.rec = 1,*SOURCE,       ) ? PRVALL ? ? ? ? ? ? ? ? ? ? ? ? ? .REPLACE(.replace(YY,"[","("),"]",")") _AREA .IF(YY.rec < YY.count,+)
        ENDFOR
        
        
    ENDFOR ;KK Action List
  ENDFOR ;XX Triad Zone
