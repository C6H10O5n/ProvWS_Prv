*GROUP -->EBM Indicators  <GROUP>
;-------------------------------------------------------------
  FOREACH XX IN(EM,HF,NULL) ;Triad  
    FOREACH ZZ IN(FLB,WLB,RLB) ;Landbase
      
      ;EBM Zone: ZZ Landbase Area Totals
      ;-------------------------------------------------------------
      *OUTPUT oELChaZZ.IF(XX=NULL,,tXX)(_TH1)  Total ELC/NDR Area - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
        *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) _INVENT _AREA
      
      ;EBM Zone: ZZ 10-yr Ageclass
      ;--------------------------------------------------------
      FOREACH YY IN (2..40,2)
        *OUTPUT oELChaZZagx.EQUATION(YY * 5).IF(XX=NULL,,tXX)(_TH1)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @AGE(.EQUATION(YY - 1)..YY) _INVENT _AREA
      ENDFOR
      *OUTPUT oELChaZZagx200p.IF(XX=NULL,,tXX)(_TH1)
        *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @AGE(41.._MAXAGE) _INVENT _AREA
        
      ;EBM Zone: ZZ by State
      ;-------------------------------------------------------------
      FOREACH YY IN (#TH10Rep)
        *OUTPUT oELChaZZstYY.IF(XX=NULL,,tXX)(_TH1)   Total ELC/NDR Area - ElcUnit:ThemeBased - Landbase:ZZ - State:YY.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? YY ZZ ZZ ? ? .IF(XX=NULL,?,XX) _INVENT _AREA
      ENDFOR


      ;Development Class <GROUP>
      ;-------------------------------------------------------------
          
        ;Base Classes
        *OUTPUT oEstZZ.IF(XX=NULL,,tXX)(_TH1)  Area in the Establishment Development Class - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yDevCls,1) _INVENT _AREA
        *OUTPUT oYngZZ.IF(XX=NULL,,tXX)(_TH1)  Area in the Young Development Class - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yDevCls,2) _INVENT _AREA
        *OUTPUT oMatZZ.IF(XX=NULL,,tXX)(_TH1)  Area in the Mature Development Class - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yDevCls,3..4) _INVENT _AREA
        *OUTPUT oOldZZ.IF(XX=NULL,,tXX)(_TH1)  Area in the  Multi-Aged/Old Forest Development Class - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yDevCls,5) _INVENT _AREA
        
        ;MAT Split 1,2 {40-80; 80-120}
        *OUTPUT oMt1ZZ.IF(XX=NULL,,tXX)(_TH1)  Area in the Mature-1 Development Class - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yDevCls,3) _INVENT _AREA
        *OUTPUT oMt2ZZ.IF(XX=NULL,,tXX)(_TH1)  Area in the Mature-2 Development Class - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yDevCls,4) _INVENT _AREA
          
        ;MAT+OLD
        *OUTPUT oMtoZZ.IF(XX=NULL,,tXX)(_TH1)  Area in the Combined Mature1 & Mature2 & Multi-Aged/Old Forest Development Class - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yDevCls,3..5) _INVENT _AREA

        ;EST+YNG
        *OUTPUT oEsyZZ.IF(XX=NULL,,tXX)(_TH1)  Area in the Combined Establishment & Young Forest Development Class - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yDevCls,1..2) _INVENT _AREA
        
        ;Crown / Private distinction
        FOREACH WW IN(CRN,PRV,NULL) 
        
          *OUTPUT oOldWWZZ.IF(XX=NULL,,tXX)(_TH1)  Area in the  Multi-Aged/Old Forest Development Class - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
            *SOURCE ? .IF(WW=NULL,?,WWAll) ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yDevCls,5) _INVENT _AREA

          *OUTPUT oMtoWWZZ.IF(XX=NULL,,tXX)(_TH1)  Area in the  Combined Mature1 & Mature2 & Multi-Aged/Old Forest Development Class - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
            *SOURCE ? .IF(WW=NULL,?,WWAll) ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yDevCls,3..5) _INVENT _AREA
            
        ENDFOR
          
        ;Development Class Percents
        *OUTPUT oOldPZZ.IF(XX=NULL,,tXX)(_TH1) %Old - ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE oOldZZ.IF(XX=NULL,,tXX) / oELChaZZ.IF(XX=NULL,,tXX) * 100
        *OUTPUT oMtoPZZ.IF(XX=NULL,,tXX)(_TH1) %Mto - ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE oMtoZZ.IF(XX=NULL,,tXX) / oELChaZZ.IF(XX=NULL,,tXX) * 100
        *OUTPUT oEsyPZZ.IF(XX=NULL,,tXX)(_TH1) %Esy - ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE oEsyZZ.IF(XX=NULL,,tXX) / oELChaZZ.IF(XX=NULL,,tXX) * 100
      
      ;<ENDGROUP> Development Class
      
      
      ;Seral Stage Class <GROUP>
      ;-------------------------------------------------------------

        ;Base Classes
        *OUTPUT oEarZZ.IF(XX=NULL,,tXX)(_TH1)     Area in an Early Seral Stage - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(ySerDevCls,13..15) _INVENT _AREA
        *OUTPUT oMidZZ.IF(XX=NULL,,tXX)(_TH1)     Area in an Mid Seral Stage - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(ySerDevCls,23..25) _INVENT _AREA
        *OUTPUT oLatZZ.IF(XX=NULL,,tXX)(_TH1)     Area in an Late Seral Stage - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(ySerDevCls,33..35) _INVENT _AREA

          ;Crown
          *OUTPUT oLatCrnZZ.IF(XX=NULL,,tXX)(_TH1)     Area in an Late Seral Stage - ElcUnit:ThemeBased - Landbase:XX - Owner:Crown
            *SOURCE ? CRNALL ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(ySerDevCls,33..35) _INVENT _AREA
          ;Private
          *OUTPUT oLatPrvZZ.IF(XX=NULL,,tXX)(_TH1)     Area in an Late Seral Stage - ElcUnit:ThemeBased - Landbase:XX - Owner:Private
            *SOURCE ? PRVALL ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(ySerDevCls,33..35) _INVENT _AREA
            
        ;Seral Class Percents
        *OUTPUT oLatPZZ.IF(XX=NULL,,tXX)(_TH1) %Lat - ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE oLatZZ.IF(XX=NULL,,tXX) /  oMtoZZ.IF(XX=NULL,,tXX) * 100

      ;<ENDGROUP> Seral Class


      ;Forest Community <GROUP>
      ;-------------------------------------------------------------

        *OUTPUT oHIHwZZ.IF(XX=NULL,,tXX)(_TH1)       Area in a HIHw(101) Forest Community - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yForComm,101) _INVENT _AREA
        *OUTPUT oHITHwZZ.IF(XX=NULL,,tXX)(_TH1)      Area in a HITHw(102) Forest Community - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yForComm,102) _INVENT _AREA
        *OUTPUT oHTHwZZ.IF(XX=NULL,,tXX)(_TH1)       Area in a HTHw(103) Forest Community - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yForComm,103) _INVENT _AREA
        *OUTPUT oMIHwHSZZ.IF(XX=NULL,,tXX)(_TH1)     Area in a MIHwHS(201) Forest Community - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yForComm,201) _INVENT _AREA
        *OUTPUT oMIHwSHZZ.IF(XX=NULL,,tXX)(_TH1)     Area in a MIHwSH(202) Forest Community - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yForComm,202) _INVENT _AREA
        *OUTPUT oMTHwZZ.IF(XX=NULL,,tXX)(_TH1)       Area in a MTHw(203) Forest Community - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yForComm,203) _INVENT _AREA
        *OUTPUT oSrSbSDomZZ.IF(XX=NULL,,tXX)(_TH1)   Area in a SrSbSDom(301) Forest Community - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yForComm,301) _INVENT _AREA
        *OUTPUT oSwSDomZZ.IF(XX=NULL,,tXX)(_TH1)     Area in a SwSDom(302) Forest Community - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yForComm,302) _INVENT _AREA
        *OUTPUT oSbFDomZZ.IF(XX=NULL,,tXX)(_TH1)     Area in a SbFDom(303) Forest Community - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yForComm,303) _INVENT _AREA
        *OUTPUT oSSpbFDomZZ.IF(XX=NULL,,tXX)(_TH1)   Area in a SSpbFDom(304) Forest Community - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yForComm,304) _INVENT _AREA
        *OUTPUT oSPiDomZZ.IF(XX=NULL,,tXX)(_TH1)     Area in a SPiDom(305) Forest Community - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yForComm,305) _INVENT _AREA
        *OUTPUT oSMHePiSpZZ.IF(XX=NULL,,tXX)(_TH1)   Area in a SMHePiSp(306) Forest Community - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yForComm,306) _INVENT _AREA
        *OUTPUT oUnkForCommZZ.IF(XX=NULL,,tXX)(_TH1) Area in an Undetermined Forest Community(0) - ElcUnit:ThemeBased - Landbase:ZZ.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? ? ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @YLD(yForComm,0) _INVENT _AREA

      ;<ENDGROUP> Forest Community
      
      
