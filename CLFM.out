*GROUP EBM Indicator Percentages By ER/NDR  <GROUP>
;-------------------------------------------------------------
  FOREACH ZZ IN (#opERs)
    FOREACH XX IN (#opZZEUs)
      FOREACH YY IN (FLB,WLB,RLB) ;Landbase
        FOREACH WW IN (CRN,PRV,NULL); Crown / Pritate distinction

            *OUTPUT oELCha.IF(WW=NULL,,WW)YYXX  XX - YY
              *SOURCE XX .IF(WW=NULL,?,WWAll) ? ? ? ? ? ? ? ? YY YY ? ? ? _INVENT _AREA

            *OUTPUT oOld.IF(WW=NULL,,WW)YYXX OLD - XX - YY
              *SOURCE XX .IF(WW=NULL,?,WWAll) ? ? ? ? ? ? ? ? YY YY ? ? ? @YLD(yDevCls,5) _INVENT _AREA
            
            *OUTPUT oM2o.IF(WW=NULL,,WW)YYXX  M2O - XX - Landbase:YY
              *SOURCE XX .IF(WW=NULL,?,WWAll) ? ? ? ? ? ? ? ? YY YY ? ? ? @YLD(yDevCls,4..5) _INVENT _AREA
            
            *OUTPUT oMto.IF(WW=NULL,,WW)YYXX  MTO - XX - Landbase:YY
              *SOURCE XX .IF(WW=NULL,?,WWAll) ? ? ? ? ? ? ? ? YY YY ? ? ? @YLD(yDevCls,3..5) _INVENT _AREA
            
            *OUTPUT oLat.IF(WW=NULL,,WW)YYXX  LAT - XX - Landbase:YY
              *SOURCE XX .IF(WW=NULL,?,WWAll) ? ? ? ? ? ? ? ? YY YY ? ? ? @YLD(ySerDevCls,33..35) _INVENT _AREA

            *OUTPUT oOldYYP.IF(WW=NULL,,WW)XX %OLD - XX - YY
              *SOURCE oOld.IF(WW=NULL,,WW)YYXX / oELCha.IF(WW=NULL,,WW)YYXX * 100
              
            *OUTPUT oM2oYYP.IF(WW=NULL,,WW)XX %M2O - XX - YY
              *SOURCE oM2o.IF(WW=NULL,,WW)YYXX / oELCha.IF(WW=NULL,,WW)YYXX * 100
              
            *OUTPUT oMtoYYP.IF(WW=NULL,,WW)XX %MTO - XX - YY
              *SOURCE oMto.IF(WW=NULL,,WW)YYXX / oELCha.IF(WW=NULL,,WW)YYXX * 100
              
            *OUTPUT oLatYYP.IF(WW=NULL,,WW)XX %LAT - XX - YY
              *SOURCE oLat.IF(WW=NULL,,WW)YYXX / oELCha.IF(WW=NULL,,WW)YYXX * 100
              
            
        ENDFOR
      ENDFOR
    ENDFOR
  ENDFOR
;<ENDGROUP> EBM Indicator Percentages By ER/NDR



*GROUP Wildlife Indicators for Wildlife Values  ;<GROUP>
;-------------------------------------------------------------
  FOREACH XX IN(EM,HF,NULL) ;Triad
    FOREACH ZZ IN (FLB,WLB,RLB) ;Landbase
          
        ;Combined Moose/DWA/Marten
        ;--------------------------------------------------------
        *OUTPUT oWildlifeHaZZ.IF(XX=NULL,,tXX) Total ZZ Area Within a Wildlife (Moose,DWA,Marten) Zone - ha.IF(XX=NULL, , Triad Zone XX)
          *SOURCE ? ? ? ? wMoose  ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) _INVENT _AREA +
                  ? ? ? ? wDWA    ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) _INVENT _AREA +
                  ? ? ? ? wMarten ? ? ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) _INVENT _AREA       
        FOREACH WW IN (PRV,CRN,NULL)
        
            ;Moose
            *OUTPUT oMooseHaZZ.IF(WW=NULL,,WW)Swd.IF(XX=NULL,,tXX)(_TH1) Area Within Moose Zone of Total Softwood on Working Landbase: By Site - ha, Owner: Crown
              *SOURCE ? .IF(WW=NULL,?,WWAll) ? ? wMoose ? SWD ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) _INVENT _AREA
            *OUTPUT oMooseHaZZ.IF(WW=NULL,,WW)MatSwd.IF(XX=NULL,,tXX)(_TH1) Area Within Moose Zone of Mature Softwood on Working Landbase: By Site - ha, Owner: Crown
              *SOURCE ? .IF(WW=NULL,?,WWAll) ? ? wMoose ? SWD ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @AGE(8.._MAXAGE) _INVENT _AREA

            ;Deer
            *OUTPUT oDeerHaZZ.IF(WW=NULL,,WW)Swd.IF(XX=NULL,,tXX)(_TH1) Area Within Deer Zone of Total Softwood on Working Landbase - ElcUnit:ThemeBased - ha, Owner: Crown
              *SOURCE ? .IF(WW=NULL,?,WWAll) ? ? wDWA ? SWD ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) _INVENT _AREA
            *OUTPUT oDeerHaZZ.IF(WW=NULL,,WW)MatSwd.IF(XX=NULL,,tXX)(_TH1) Area Within Deer Zone of Mature Softwood on Working Landbase -ElcUnit:ThemeBased - ha, Owner: Crown
              *SOURCE ? .IF(WW=NULL,?,WWAll) ? ? wDWA ? SWD ? ? ? ZZ ZZ ? ? .IF(XX=NULL,?,XX) @AGE(8.._MAXAGE) _INVENT _AREA
            
        ENDFOR ;WW Ownership 

   ENDFOR ;ZZ Landbase
  ENDFOR ;XX Triad Zone
;<ENDGROUP> Wildlife 
  
  
  
